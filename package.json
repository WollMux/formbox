  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --ssl 1 --disable-host-check --sourcemaps=true",
    "build": "ng build && ng lint",
    "build-jenkins": "ng build && ng lint --format json > lint.json",
    "test": "ng test",
    "test-jenkins": "ng test --config karma-jenkins.conf.js --code-coverage --progress false",
    "lint": "ng lint --t verbose",
    "e2e": "ng e2e --progress false",
    "nooffice": "ng serve --host 0.0.0.0 --ssl 1 --disable-host-check --sourcemaps=true --environment=test",
    "sonar": "sonarqube-scanner-node",
    "webdriver-update": "webdriver-manager clean && webdriver-manager update",
    "docs": "npm run typedoc -- --hideGenerator --exclude **/*.spec.ts ./src/",
    "typedoc": "typedoc",
    "gen-redux": "ribosomejs ribosome/redux.js.dna"
  },
  "private": true,
